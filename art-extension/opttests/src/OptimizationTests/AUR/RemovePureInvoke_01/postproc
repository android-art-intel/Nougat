packagedir=`echo "$(cd $(dirname $0); pwd)/../"`

declare -a testMethodsCounts=(\
        "runTest:Environment of .* NullCheck@.* no longer uses the value defined by .* IntConstant==1" \
        "runTest:Environment of .* NullCheck@.* no longer uses the value defined by .* InvokeStaticOrDirect==1" \
        "runTest:Environment of .* NullCheck@.* no longer uses the value defined by .* ParameterValue==7" \
        "runTest:Environment of .* NullCheck@.* no longer uses the value defined by .* Phi==1" \
        "runTest:Environment of .* SuspendCheck@.* no longer uses the value defined by .* ParameterValue==6" \
        "runTest:Environment of .* SuspendCheck@.* still uses the value defined by .* ParameterValue==1" \
        "runTest:Removing .* InvokeStaticOrDirect@.* because it is no longer used by any other instruction or environment.==1" \
        "runTest:Removing .* Phi@.* because it is no longer used by any other instruction or environment.==1" \
        )
declare -a logcatLinesNumber=("runTest:21")

# include the package's postproc
if [ -f $packagedir/postproc_common ]; then
   source $packagedir/postproc_common
fi

